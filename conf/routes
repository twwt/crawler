# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                    controllers.Application.index
GET         /submit              controllers.Application.submit
GET         /upload              controllers.Application.upload
POST        /upload              controllers.Application.postUpload
GET         /face/upload         controllers.Application.faceUploadForm
POST        /face/upload         controllers.Application.faceUpload
# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.at(path="/public", file)

    